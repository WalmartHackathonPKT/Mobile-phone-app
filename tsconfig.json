{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["expo-file-system"],
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "isolatedModules": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
