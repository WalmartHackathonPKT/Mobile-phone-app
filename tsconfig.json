{
  "compilerOptions": {
    "types": [
      "expo-file-system"
    ],
    "jsx": "react-jsx",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "esModuleInterop": true,
    "isolatedModules": false
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "try.js"  ],
  "extends": "expo/tsconfig.base"
}
